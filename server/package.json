  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "set NODE_ENV=production && nodemon index.ts",
    "dev": "set NODE_ENV=local&& concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npx eslint . --ext .js,.ts --fix --ignore-path .gitignore || true",
    "format": "prettier . --write --ignore-path .gitignore",
    "lintformat": "npm run lint && npm run format"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^6.9.1",
    "prettier": "^2.8.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.11.19",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.20",
    "pre-commit": "^1.2.2",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "_moduleAliases": {
    "@": "dist/src"
